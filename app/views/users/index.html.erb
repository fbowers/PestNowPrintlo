<h1>Listing rusers</h1>

<table>
  <tr>
    <th>Userid</th>
    <th>Login</th>
    <th></th>
    <th></th>
    <th></th>
     <th>Crypted password</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.UserId %></td>
    <td><%= user.login %></td>
    <td><%= link_to 'Show', user, :confirm => 'Are you sure?' %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
    <td><%= user.crypted_password %></td>
   
  </tr>
<% end %>
</table>

