<h1>Editing property</h1>

<% form_for(@property) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :PropertyId %><br />
    <%= f.text_field :PropertyId %>
  </p>
  <p>
    <%= f.label :AddressId %><br />
    <%= f.text_field :AddressId %>
  </p>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @property %> |
<%= link_to 'Back', properties_path %>