<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN"
"http://www.w3.org/TR/html4/strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<title>PestNow</title>

		<%= javascript_include_tag 'jq/jquery-1.4.2.min.js','jq/themes/southstreet/jquery-ui-1.8.2.custom.min.js'%>
		<%= javascript_include_tag 'jq/jquery.ui.tabs.js'%>
		<%= javascript_include_tag 'jq/plug/jquery.dataTables.js'%>
		<%= javascript_include_tag 'jq/plug/autoNumeric-1.6.1.js'%>		
		<%= javascript_include_tag 'pn/main.js', "custom-scripting.js"%>

		<%= stylesheet_link_tag 'south-street/jquery-ui-1.8.2.custom'%>
		<%= stylesheet_link_tag 'formtastic'%>
		<%= stylesheet_link_tag 'datatable'%>
		<%= stylesheet_link_tag 'main'%>
                <%= stylesheet_link_tag 'print', :media => 'screen, print' %>
<script type="text/javascript" src="http://code.jquery.com/mobile/1.0a3/jquery.mobile-1.0a3.min.js"></script>

        <link rel="stylesheet" href="http://code.jquery.com/mobile/1.0a3/jquery.mobile-1.0a3.min.css" />

		<script type="text/javascript">
		jQuery(document).ready(function(){
			PN.setupPage();
			jQuery('#search_property').focus();
			jQuery('#search_property').blur(function(){if ($('#search_property').val()) {$('#form-search-prop').submit();}});
		});
		
		</script>

	</head>
	<body>
		<div id='main'>
			
			<div id='content-holder' class='normal'>
				<div id="content">
					<%=yield%>
				</div>
			</div>
			<div id='todo'>
			</div>
			<div id="busy">Working...</div>
			<div id="msg"></div>
			<div id="dialog"></div>
		</div>

	</body>
</html>
