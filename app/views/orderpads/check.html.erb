<p id="notice"><%= notice %></p>

 <td><%=@orderpad.Servicecode.Code %></td>

<p>
  <b>Orderid:</b>
  <%= @orderpad.OrderId %>
</p>
<%= render :partial => 'orders/propaddress', :locals => { :propaddress => @orderpad }, :as => :o%>
<div id=invbilladd>
  <% if @orderpad.BillBillingCustomer == true %>
<%= render :partial => 'orders/propbilladdress', :locals => { :propbilladdress => @orderpad }, :as => :ba%>
<%elsif @orderpad.BillSettlementCompany == true%>
<%= render :partial => 'orders/propsettleaddress',  :locals => { :propsettleaddress => @orderpad },:as => :set%>
<%elsif @orderpad.BillAgent == true%>
<%= render :partial => 'orders/propagentaddress', :locals => { :propagentaddress => @orderpad }, :as => :agn%>
<%else%>
<%= render :partial => 'orders/propaddress', :locals => { :propaddress => @orderpad }, :as => :o%>
<% end %></div>

<div id="woagentinfo">
<% if @orderpad.Ragent.nil?%>
<%= "No Agent" %>
<%else%>
<%= render :partial => 'orders/propagentaddress', :collection => b.Orders, :as => :agn%>
<%end%>
</div>
<div id="wosettlementinfo">
<% if @orderpad.Rsettlement.nil?%>
<%= "No Settle" %>
<%else%>
<%= render :partial => 'orders/propsettleaddress', :collection => b.Orders, :as => :set%>
<%end%>
</div>


