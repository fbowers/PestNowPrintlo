<h1>New address</h1>

<% form_for(@address) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :AddressId %><br />
    <%= f.text_field :AddressId %>
  </p>
  <p>
    <%= f.label :StreetNumber %><br />
    <%= f.text_field :StreetNumber %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', addresses_path %>